{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["**/*.ts"],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "ecmaVersion": 5,
        "project": "./tsconfig.json"
      },
      "plugins": ["@typescript-eslint"],
      "extends": ["airbnb-typescript/base", "prettier"],
      "rules": {
        "import/prefer-default-export": 0,
        "no-console": 0,
        "no-param-reassign": [
          "error",
          {
            "props": false
          }
        ]
      }
    }
  ]
}
